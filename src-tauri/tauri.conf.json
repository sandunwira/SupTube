{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "../src",
    "distDir": "../src",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "SupTube",
    "version": "1.5.1"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "window": {
        "all": false,
        "print": false,
        "setFullscreen": true,
        "setResizable": true
      }
    },
    "bundle": {
      "active": true,
      "publisher": "Sandun Wiratunga",
      "category": "Entertainment",
      "copyright": "Sandun Wiratunga",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.suptube.app",
      "shortDescription": "SupTube",
      "longDescription": "An opensource client for YouTube",
      "resources": [],
      "externalBin": [],
      "targets": "all",
      "deb": {
        "depends": []
      },
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "appimage": {
        "bundleMediaFramework": true
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": "",
        "allowDowngrades": false,
        "wix": {
          "language": "en-US",
          "dialogImagePath": "../src/assets/dialogImage.bmp",
          "bannerPath": "../src/assets/banner.bmp"
        }
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "dialog": true,
      "endpoints": [
        "https://raw.githubusercontent.com/sandunwira/TauriUpdater/main/suptube.json"
      ],
      "windows": {
        "installMode": "basicUi"
      },
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDQ1MDlEMEJCQ0ZGNEQ1MzUKUldRMTFmVFB1OUFKUlZiVUFzMkhYbUR2VWh3V0Z4UzdoT29FSURNQ0JKQUx5TEt4bjZRd05KY2IK"
    },
    "windows": [
      {
        "title": "SupTube",
        "minWidth": 1400,
        "minHeight": 700,
        "url": "https://www.youtube.com",
        "fullscreen": false,
        "focus": true,
        "resizable": true,
        "decorations": true,
        "center": true
      }
    ]
  }
}
